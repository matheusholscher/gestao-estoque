<p id="notice"><%= notice %></p>

<h1>Operacaos</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @operacaos.each do |operacao| %>
      <tr>
        <td><%= operacao.descricao %></td>
        <td><%= operacao.tipo %></td>
        <td><%= link_to 'Show', operacao %></td>
        <td><%= link_to 'Edit', edit_operacao_path(operacao) %></td>
        <td><%= link_to 'Destroy', operacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Operacao', new_operacao_path %>
